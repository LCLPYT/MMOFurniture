  "authors": [
    "MrCrayfish",
    "LCLP"
  ],
  "contact": {
    "homepage": "https://github.com/LCLPYT/MMOFurniture",
    "sources": "https://github.com/LCLPYT/MMOFurniture"
  },

  "license": "GPL v3",
  "icon": "assets/cfm/icon.png",

  "environment": "*",
